{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/auth/register.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/auth/login.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/user/[id].js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "pages/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "css/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "script/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/cursos", "dest": "/pages/courses.html", "status": 200 },
    { "src": "/ebooks", "dest": "index.html", "status": 200 },
    { "src": "/produtos", "dest": "/pages/products.html", "status": 200 },
    { "src": "/log-in", "dest": "/pages/log-in.html", "methods": ["POST"], "status": 200 },
    { "src": "/sign-in", "dest": "/pages/sign-in.html", "methods": ["POST"], "status": 200 }
  ]
}
